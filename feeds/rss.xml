---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.name | xml_escape }}</title>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}{{ page.url }}" rel="self" type="application/rss+xml" />
    {% if site.description %}<description>{{ site.description | xml_escape }}</description>{% endif %}
    <language>en-au</language>
    <pubDate>{{ site.posts[0].date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>
    {% for post in site.posts limit: 20 %}
    <item>
      <title>{{ post.title | xml_escape }}</title>
      <link>{{ site.url }}{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>{{ post.authors[0].email }} ({{ post.authors[0].dislay_name }}){% if post.authors.size > 1 %} and others{% endif %}</author>
      <guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}
  </channel> 
</rss>
